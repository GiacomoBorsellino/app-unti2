generator client {
  provider = "prisma-client-js"
  output   = "./generated/client_secondary"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_SECONDARY")
  // schemas  = ["secondary"]
}

model test_table_secondary {
  id      Int     @id @default(autoincrement())
  name    String? @db.VarChar(255)
  del     Boolean @default(false)
  field_3 Int     @default(3)

  // @@schema("secondary")
}
